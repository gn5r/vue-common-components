{
  "name": "@gn5r/vue-common-components",
  "version": "0.1.0-beta.0",
  "private": false,
  "author": "gn5r",
  "description": "vuetify拡張コンポーネントライブラリ",
  "main": "dist/vue-common-components.common.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:components": "vue-cli-service build --target lib --name components src/build/build.components.js",
    "build:lib": "vue-cli-service build --target lib --name vue-common-components src/build/build.lib.js",
    "lint": "vue-cli-service lint",
    "gen:json": "node ./src/build/build.vetur.js"
  },
  "dependencies": {},
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.5",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue": "^2.6.11",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.2.11",
    "vuetify-loader": "^1.3.0"
  },
  "vetur": {
    "tags": "dist/vetur/tags.json",
    "attributes": "dist/vetur/attributes.json"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gn5r/vue-common-components.git"
  },
  "homepage": "https://gn5r.github.io/vue-common-components/",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "public"
  }
}
